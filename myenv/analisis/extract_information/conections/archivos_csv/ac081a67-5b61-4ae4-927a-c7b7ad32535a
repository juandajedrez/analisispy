@article{BERTOLACCINI2024107990,
title = {Segmentectomy vs. Lobectomy in stage IA non-small cell lung cancer: A systematic review and meta-analysis of perioperative and survival outcomes},
journal = {Lung Cancer},
volume = {197},
pages = {107990},
year = {2024},
issn = {0169-5002},
doi = {https://doi.org/10.1016/j.lungcan.2024.107990},
url = {https://www.sciencedirect.com/science/article/pii/S0169500224005245},
author = {Luca Bertolaccini and Antonino Carmelo Tralongo and Marzia {Del Re} and Francesco Facchinetti and Roberto Ferrara and Tindara Franchina and Paolo Graziano and Umberto Malapelle and Jessica Menis and Antonio Passaro and Sara Pilotto and Sara Ramella and Giulio Rossi and Rocco Trisolini and Michela Cinquini and Francesco Passiglia and Silvia Novello},
keywords = {Lung cancer, Segmentectomy, Lobectomy, Systematic Review, Meta-Analysis},
abstract = {While recent randomized controlled trials (RCT) have suggested superior overall survival (OS) outcomes with segmentectomy over lobectomy, questions remain regarding the comparability of these surgical procedures for treating early-stage non-small cell lung cancer (NSCLC). This systematic review and meta-analysis aimed to synthetize existing evidence and to compare the survival outcomes observed for stage IA NSCLC following segmentectomy or lobectomy. 40 studies (38 observational, 2 RCTs) encompassing 103,926 patients were analyzed. Primary outcomes included overall survival (OS), disease-free survival (DFS), local recurrences, harvested lymph nodes, postoperative morbidity, and length of hospital stay. Risk of bias was assessed using established tools, and evidence certainty was evaluated using GRADE. Non-RCTs showed an OS HR of 1.10 (95 % CI: 0.94–1.30, p = 0.24) with low certainty, contrasting with RCTs’ HR of 0.82 (95 % CI: 0.66–1.02, p = 0.7) with moderate certainty. Local recurrences exhibited OR 1.40 (95 % CI: 0.94–2.08, p = 0.09) in non-RCTs with low certainty, and RR 1.61 (95 % CI: 1.12–2.31, p = 0.01) in RCTs with low certainty. Non-RCTs showed DFS HR 1.13 (95 % CI: 0.95–1.34, p = 0.18) with low certainty, while RCTs yielded HR 1.00 (95 % CI: 0.85–1.18, p = 0.97) with moderate certainty. Lobectomy resulted in more harvested lymph nodes. Postoperative morbidity and length of hospital stay did not differ significantly. While definitive evidence for OS, DFS, and postoperative outcomes differences was inconclusive, a potential increase in local recurrences following lobectomy was noted. Further well-designed studies are warranted to enhance evidence and inform clinical practice in stage I lung cancer surgery.}
}
@article{ARNSON2023108,
title = {Genome editing using Staphylococcus aureus Cas9 in a canine model of glycogen storage disease Ia},
journal = {Molecular Therapy - Methods & Clinical Development},
volume = {29},
pages = {108-119},
year = {2023},
issn = {2329-0501},
doi = {https://doi.org/10.1016/j.omtm.2023.03.001},
