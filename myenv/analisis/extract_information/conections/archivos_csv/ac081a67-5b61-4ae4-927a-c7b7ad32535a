title = {Escin Ia Ameliorates DSS-Induced Chronic Colitis in Mice by Inhibiting Inflammation and Oxidative Stress via the LOXL2/MMP-9 Pathway},
journal = {Journal of Ethnopharmacology},
pages = {119623},
year = {2025},
